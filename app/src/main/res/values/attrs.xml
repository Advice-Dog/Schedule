<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="dark_mode" format="string" />

    <attr name="footer_label" format="string" />
    <attr name="footer_icon" format="reference" />
    <attr name="footer_tint" format="reference" />

    <attr name="border_tint" format="reference" />
    <attr name="menu_tint" format="reference" />
    <attr name="filter_background" format="reference" />
    <attr name="category_tint" format="reference" />

    <declare-styleable name="EmptyView">
        <attr name="evIcon" format="reference" />
        <attr name="evTitle" format="reference" />
        <attr name="evMessage" format="reference" />
    </declare-styleable>

    <declare-styleable name="SkullView">
        <attr name="skullViewDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="EventView">
        <attr name="eventView_showDot" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GlitchContainerView">
        <attr name="glitch_isGlitch" format="boolean" />
        <attr name="glitch_corruptionLevel" format="enum">
            <enum name="NONE" value="0" />
            <enum name="MINOR" value="1" />
            <enum name="MEDIUM" value="2" />
            <enum name="MAJOR" value="3" />
        </attr>
    </declare-styleable>


    <declare-styleable name="NavigationView">
        <attr name="navigationLabel" format="reference" />
    </declare-styleable>

    <declare-styleable name="WellView">
        <attr name="wellView_hasDismiss" format="boolean" />
    </declare-styleable>

</resources>